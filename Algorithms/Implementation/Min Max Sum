using namespace std;


int main(){
    long int arr[5];
    int i;
    for (i = 0; i < 5; i++){
        cin >> arr[i];
    }
    sort (arr, arr+5);
    cout << (arr[0] + arr[1] + arr[2] + arr[3]) << " " << (arr[1] + arr[2] + arr[3] + arr[4]);
    
    return 0;
}
