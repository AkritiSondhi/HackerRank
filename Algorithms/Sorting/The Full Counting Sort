#include <bits/stdc++.h>

using namespace std;

int main() {
    long int n;
    cin >> n;
    
    string result[100];
    
    for(long int i = 0; i < n/2; i++) {
        int index;
        cin >> index;
        
        string s;
        cin >> s;
        
        result[index] = result[index] + "- ";
    }

    
    for(long int i = n/2; i < n; i++) {
        int index;
        cin >> index;
        
        string s;
        cin >> s;

        result[index] = result[index] + s + " ";
    }
    
    for(int i = 0; i < 100; i++) {
        cout << result[i];
    }

    return 0;
}
