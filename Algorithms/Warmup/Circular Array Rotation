#include <map>
#include <set>
#include <list>
#include <cmath>
#include <ctime>
#include <deque>
#include <queue>
#include <stack>
#include <string>
#include <bitset>
#include <cstdio>
#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>

using namespace std;


int main(){
    int n;
    int k;
    int q;
    cin >> n >> k >> q;
    vector<int> a(n);
    int rotation;
    int index;
    
    for(int a_i = 0;a_i < n;a_i++){
        cin >> a[a_i]; //array to be rotated
    }
    
    rotation = k % n;

    for(int a0 = 0; a0 < q; a0++){
        int m;
        cin >> m; //value for q        
        if (m - rotation >= 0){
            cout << a[m - rotation]<< endl;
        }
        else{
            cout << a[m - rotation + n]<< endl;
        }
    }
    return 0;
}
