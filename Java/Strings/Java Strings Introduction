import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        
        System.out.println(A.length() + B.length());
        System.out.println(A.compareTo(B) > 0 ? "Yes" : "No");
        String combined1 = A.toUpperCase().charAt(0) + A.substring(1);
        String combined2 = B.toUpperCase().charAt(0) + B.substring(1);
        System.out.println(combined1 + " " + combined2);
    }
}



