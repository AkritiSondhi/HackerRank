import java.io.*;
import java.util.*;
import java.math.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        BigInteger sum = new BigInteger("0");

        for (int i = 0; i < n; i++) {
            String num = in.next();
            sum = sum.add(new BigInteger(num));
        }

        System.out.println(sum.toString().substring(0, 10));
    }
}

